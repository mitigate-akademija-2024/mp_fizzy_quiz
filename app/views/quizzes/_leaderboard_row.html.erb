<% if score %>
  <tr>
    <% if current_user != score.user %>
      <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= score.user.username ? score.user.username : "Anonymous User" %></th>
    <% else %>
      <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><strong>You</strong></th>
    <% end %>
    <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= score.correct_count %></th>
    <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= format_date score.created_at %></th>
  </tr>
<% end %>

<% if aggregrate %>
  <tr>
    <% if current_user != aggregrate.user %>
      <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= aggregrate.user.username ? aggregrate.user.username : "Anonymous User" %></th>
    <% else %>
      <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><strong>You</strong></th>
    <% end %>
    <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= aggregrate.total_quizzes %></th>
    <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= aggregrate.total_correct %></th>
    <th class="whitespace-nowrap px-4 py-2 font-medium text-gray-900"><%= format_date aggregrate.last_date %></th>
  </tr>
<% end %>